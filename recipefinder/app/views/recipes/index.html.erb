<h1><%=@search_term%> recipes</h1>
<table>
	<tr>
		<th>Image</th>
		<th>Title</th>
		<th>Social Rank</th>
	</tr>
	<% @recipes.each do |recipe| %>
	<tr>
		<td> <%= link_to image_tag(recipe["image_url"], size:"100x100"), recipe["f2f_url"]%></td>
		<td> <%= sanitize link_to  recipe["title"], recipe["f2f_url"]%> </td>
		<td> <%= recipe["social_rank"]%></td>
	</tr>
	<% end %>
</table>